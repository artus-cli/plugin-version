{
  "name": "@artus-cli/plugin-version",
  "version": "0.0.1",
  "description": "version plugin for artus cli",
  "homepage": "",
  "author": "",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "types": "dist/index.d.ts",
